{
  "name": "coursify-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "A React frontend for the Coursify schedule building application",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
